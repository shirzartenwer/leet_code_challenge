class Solution:
    def isSubsequence(self, s: str, t: str) -> bool:
        if len(s) ==0:
            return True
        if len(t) < len(s):
            return False

        j = 0 
        for index in range(len(t)):
            if t[index] == s[j]:
                j+=1
                if j == len(s):
                    return True
        else:
            return False


class Solution:
    def isSubsequence(self, s: str, t: str) -> bool:
        if len(s) ==0:
            return True
        if len(t) < len(s):
            return False

        i =0 
        j = 0
        while i < len(s) and j < len(t):
            if t[j] == s [i]:
                i +=1
            j+=1
        # This return line convers the cases when s is emtpy string and if s is longer than t.
        return i == len(s)



Solution().isSubsequence("abc", "ahbgdc")